/**index.wxss**/

swiper {
  align-items: flex-end;
}

swiper image {
  width: 100%;
}

/** banner**/

.head {
  z-index: 1;
  position: fixed;
  top: 0rpx;
  width: 100%;
}

.slide-image {
  width: 100%;
}

.app-item {
  /* margin: 10px auto 0 auto; */
  width: 25%;
  display: flex;
  flex-direction: row;
  /* justify-content: space-between; *//* align-items: center; *//* height: 100px; *//* box-shadow:2px 2px 5px #e1e1e1; */
  border-radius: 5px;
  margin-top:15rpx;

}

.app-item-content {
  /* display: flex; *//* flex-direction: row; *//* align-items: center; */
  flex: 1;
}

.content {
  display: flex;
  flex-direction: row;
  width: 90%;
  padding-bottom: 20rpx;
  border-bottom: 0.5px solid rgb(231, 231, 231);
  flex-wrap: wrap;
  margin: auto;
  margin-bottom: 10rpx;
}

.div {
  float: left;
}

.divs image {
  width: 30rpx;
  height: 36rpx;
  float: left;
  margin-left: 50rpx;
  margin-top: 5rpx;
}

.section__title {
  color: orange;
  font-size: 18px;
  float: left;
  margin-left: 18rpx;
}

.section__url {
  margin: 0 0 0 50rpx;
}

.section__url image {
  width: 34rpx;
  height: 34rpx;
  float: left;
  padding-top: 10rpx;
}

.section-color {
  color: #ffd700;
  font-weight: 700;
  display: inline;
}

.app-image {
  margin-top: 15rpx;
  /* margin-left: 17rpx; */
  margin-right: 12rpx;
  width: 130rpx;
  height: 130rpx;
  border-radius: 27rpx;
  box-shadow:darkgrey 0px 0rpx 30rpx 2rpx;
}

.content-title {
  font-size: 26rpx;
  text-align: center;
  line-height: 35rpx;
  width: 80px;
  overflow: hidden;
  height: 35rpx;
}

.content-desc {
  display: inline-block;
  color: #e4393c;
  font-weight: normal;
  font-size: 24rpx;
  margin-left: 10rpx;
}

.content-play {
  margin-top: 5rpx;
  margin-bottom: 5rpx;
  color: rgb(163, 163, 163);
  font-size: 20rpx;
  width: 80px;
  overflow: hidden;
  height: 27rpx;
  text-align: center;
  /* padding-left: 15rpx; *//* letter-spacing:1px; */
}

.start-navigator {
  margin-right: 10rpx;
  border-radius: 15rpx;
  /* background-color: #fff; */
}

.start-btn {
  color: #fff;
  height: 30px;
  line-height: 30px;
  width: 60px;
  font-size: 24rpx;
  padding: 0px 3px;
  border-radius: 40px;
  background-color: #1aad19;
}

.start-btn::after {
  border: none;
}

.promotion-container {
  margin-bottom: 10px;
  position:  relative;
}

.promotion-view {
  /* top: 0px;
  left: 0px; *//* width: 100%;
  height: 35px; *//* background-color: #CCCCCC; */
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  align-items: center;
}

.panda {
  position: fixed;
  top: 0;
  right: 40rpx;
  z-index: 2;
}

.panda image {
  display: block;
  width: 210rpx;
  height: 175rpx;
}

.share {
  position: fixed;
  bottom: 40rpx;
  right: 55rpx;
  z-index: 2;
}

button::after {
  content: "";
  border: none;
}

.share button {
  display: block;
  width: 150rpx;
  height: 70rpx;
  padding: 0;
  background: none;
  border: none;
  border-radius: 20rpx;
}

.share image {
  display: block;
  margin: 0;
  width: 150rpx;
  height: 70rpx;
}

button::after {
  content: "";
  border: none;
}

.promotion-collect {
  background-color: #1aad19;
  padding: 2px 5px;
  color: #fff;
  border-radius: 15px;
  margin-left: 20px;
}

.bg {
  display: none;
  position: absolute;
  top: 0%;
  left: 0%;
  width: 100%;
  height: 100%;
  background-color: black;
  z-index: 1001;
  -moz-opacity: 0.7;
  opacity: 0.70;
  filter: alpha(opacity=70);
}

.show {
  display: none;
  text-align: center;
  position: absolute;
  top: 0;
  left: 5%;
  /* width: 53%; *//* height: 10%; *//* padding: 8px; */
  z-index: 1002;
  overflow: auto;
}

.show image {
  display: block;
  width: 700rpx;
  height: 320rpx;
}

.txt {
  margin-top: 20rpx;
  font-size: 28rpx;
  color: royalblue;
}

.a-scroll {
  width: 100%;
  height: 300rpx;
}

.my-scroll {
  width: 100%;
  white-space: nowrap;
}

.s-item {
  width: 100%;
  display: inline-block;
}

.a-scroll image {
  width: 100%;
  height: 300rpx;
}

