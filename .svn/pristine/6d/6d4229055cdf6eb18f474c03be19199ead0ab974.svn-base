/* 游戏分享弹窗 */
.mask {
  width: 100%;
  height: 100%;
  position: fixed;
  top: 0;
  left: 0;
  background: #000;
  z-index: 50;
  opacity: 0.5;
}

.modalDlg {
  width: 500rpx;
  height: 680rpx;
  position: absolute;
  top: 580rpx;
